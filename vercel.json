{
  "version": 2,
  "builds": [
    {
      "src": "vercel_server.js",
      "use": "@vercel/node"
    },
    {
      "src": "pages/**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "vercer_server.js"
    },
    {
      "src": "/pages/(.*)",
      "dest": "/pages/$1"
    },
    {
      "src": "/(.*)",
      "dest": "vercer_server.js"
    }
  ]
}
